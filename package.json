{
  "name": "@sinohope/sinohope-web3-provider",
  "version": "1.0.0",
  "description": "EIP-1193 Compatible Ethereum provider for Sinohope",
  "repository": "github:sinohope/sinohope-web3-provider",
  "author": "Sinohope",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "wallet",
    "waas",
    "sinohope"
  ],
  "scripts": {
    "lint:fix": "prettier --write 'src/**/*.{js,ts}' 'test/**/*.{js,ts}' && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "test": "mocha --exit --recursive 'test/**/*.test.ts'",
    "testread": "mocha --exit 'test/ethers/read.test.ts'",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@sinohope/js-sdk": "^1.0.10",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "4.3.1",
    "@types/debug": "^4.1.7",
    "@types/mocha": "9.1.1",
    "@types/node-fetch": "^2.6.2",
    "chai": "4.3.6",
    "dotenv": "^16.0.3",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typechain": "^8.1.1",
    "typescript": "^4.7.4",
    "web3": "^4.1.2"
  },
  "dependencies": {
    "@ethersproject/units": "^5.7.0",
    "axios": "^1.7.4",
    "debug": "^4.3.4",
    "ethers": "^5.7.2",
    "https-proxy-agent": "^7.0.2",
    "uuid": "^10.0.0",
    "web3-providers-http": "1.8.0"
  }
}
